<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>Gender</th>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Street</th>
      <th>Plz</th>
      <th>Place</th>
      <th>Job</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Sleep</th>
      <th>Room</th>
      <th>Namecompany</th>
      <th>Streetcompany</th>
      <th>Placecompany</th>
      <th>Veggie</th>
      <th>Accept</th>
      <th>Event</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.gender %></td>
        <td><%= member.firstName %></td>
        <td><%= member.lastName %></td>
        <td><%= member.street %></td>
        <td><%= member.plz %></td>
        <td><%= member.place %></td>
        <td><%= member.job %></td>
        <td><%= member.phone %></td>
        <td><%= member.eMail %></td>
        <td><%= member.sleep %></td>
        <td><%= member.room %></td>
        <td><%= member.nameCompany %></td>
        <td><%= member.streetCompany %></td>
        <td><%= member.placeCompany %></td>
        <td><%= member.veggie %></td>
        <td><%= member.accept %></td>
        <td><%= member.event_id %></td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_member_path %>
