<p id="notice"><%= notice %></p>

<h1>Abonennten Newsletters</h1>

<table class="table">
  <thead>
    <tr>
      <th>Email</th>
      <th>Enable</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @abo_newsletters.each do |abo_newsletter| %>
      <tr>
        <td><%= abo_newsletter.eMail %></td>
        <td><%= abo_newsletter.enable %></td>
       <!-- <td><%#= abo_newsletter.newsletter %></td> -->
        <td><%= link_to 'Anzeigen', abo_newsletter %></td>
        <td><%= link_to 'Bearbeiten', edit_abo_newsletter_path(abo_newsletter) %></td>
        <td><%= link_to 'Löschen', abo_newsletter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Neuen Abonennten für den Newsletter hinzufügen', new_abo_newsletter_path %>
